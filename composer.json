{
    "name": "arkitecht/calendar",
    "description": "PHP Calendar Utilities for Drawing Calendars",
    "type": "library",
    "require": {
        "nesbot/carbon": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron J. Rubin",
            "email": "aaron@arkitech.net"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Arkitecht\\Calendar\\": "src/Arkitecht/Calendar/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
